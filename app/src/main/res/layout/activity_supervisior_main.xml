<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SupervisiorMain">
    <Button
        android:id="@+id/ongo1"
        android:layout_width="214dp"
        android:layout_height="69dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="196dp"
        android:layout_marginTop="72dp"
        android:layout_marginEnd="0dp"
        android:layout_marginRight="0dp"
        android:background="@drawable/typesofcb"
        android:text="On Going"
        android:textAllCaps="false"
        android:textSize="25dp" />

    <TextView
        android:id="@+id/title-cs"
        android:layout_width="412dp"
        android:layout_height="72dp"
        android:layout_marginStart="0dp"
        android:layout_marginTop="0dp"
        android:layout_marginEnd="0dp"
        android:background="@drawable/postcomplaint"
        android:text="Supervisor"
        android:textColor="#070003"
        android:textAlignment="center"
        android:textDirection="rtl"
        android:textSize="39dp" />


    <ImageButton
        android:id="@+id/bb2"
        android:layout_width="71dp"
        android:layout_height="55dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="6dp"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="541dp"
        android:background="@drawable/postcomplaint"
        android:src="@drawable/outline_arrow_back_black_18dp" />

    <Button
        android:layout_width="196dp"
        android:layout_height="69dp"
        android:id="@+id/pen1"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="0dp"
        android:layout_marginTop="72dp"
        android:background="@drawable/typesofcb"
        android:text="Pending"
        android:textAllCaps="false"
        android:textSize="25dp" />
</RelativeLayout>